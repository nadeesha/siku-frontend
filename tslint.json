{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "ordered-imports": [
      false
    ],
    "class-name": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "member-ordering": [
      false
    ]
  },
  "rulesDirectory": []
}